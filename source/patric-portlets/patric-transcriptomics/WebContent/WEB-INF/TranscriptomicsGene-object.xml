<deployments>
	<deployment>
		<parent-ref>patric</parent-ref>
		<if-exists>overwrite</if-exists>
		<page>
			<page-name>Test File Uploader</page-name>
			<properties>
				<property>
					<name>layout.id</name>
					<value>patric-full</value>
				</property>
			</properties>
			<window>
				<window-name>TestFileUploaderWindow</window-name>
				<instance-ref>TestFileUploaderInstance</instance-ref>
				<region>center</region>
				<height>0</height>
				<properties>
					<property>
						<name>theme.renderSetId</name>
						<value>emptyRenderer</value>
					</property>
				</properties>
			</window>
			<coordination>
				<bindings>
					<implicit-mode>FALSE</implicit-mode>
					<alias-binding>
						<id>cType</id>
						<qname>{http://patricbrc.org/params}context_type</qname>
					</alias-binding>
					<alias-binding>
						<id>cId</id>
						<qname>{http://patricbrc.org/params}context_id</qname>
					</alias-binding>
				</bindings>
			</coordination>
		</page>
	</deployment>

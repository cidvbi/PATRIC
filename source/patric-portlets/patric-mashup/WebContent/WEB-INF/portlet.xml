	<!-- public render parameters -->
	<public-render-parameter>
		<identifier>context_type</identifier>
		<qname xmlns:x="http://patricbrc.org/params">x:context_type</qname>
	</public-render-parameter>
	<public-render-parameter>
		<identifier>context_id</identifier>
		<qname xmlns:x="http://patricbrc.org/params">x:context_id</qname>
	</public-render-parameter>
	<public-render-parameter>
        <identifier>filter</identifier>
        <qname xmlns:x="http://patricbrc.org/params">x:filter</qname>
    </public-render-parameter>
    <public-render-parameter>
        <identifier>keyword</identifier>
        <qname xmlns:x="http://patricbrc.org/params">x:keyword</qname>
    </public-render-parameter>
    <public-render-parameter>
        <identifier>scope</identifier>
        <qname xmlns:x="http://patricbrc.org/params">x:scope</qname>
    </public-render-parameter>
    <public-render-parameter>
        <identifier>time</identifier>
        <qname xmlns:x="http://patricbrc.org/params">x:time</qname>
    </public-render-parameter>
        	<!-- used for jmol only -->
    <public-render-parameter>
        <identifier>pdb_id</identifier>
        <qname xmlns:x="http://patricbrc.org/params">x:pdb_id</qname>
    </public-render-parameter>
    <public-render-parameter>
        <identifier>chain_id</identifier>
        <qname xmlns:x="http://patricbrc.org/params">x:chain_id</qname>
    </public-render-parameter>
